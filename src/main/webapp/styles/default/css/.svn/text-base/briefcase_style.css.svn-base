/* clears and universal styles */

body {
    font-family: tahoma, arial, sans-serif;
    background-color: #000000;
}

body, form, input, p, ul, ol, li, h1, h2, h3, h4, h5, h6 {
    margin: 0;
    padding: 0;
}

img, a img {
