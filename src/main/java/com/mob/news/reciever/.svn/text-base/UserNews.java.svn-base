public class UserNews {

    private long newsOwner;
    private String news;

    public UserNews(String news, long newsOwner) {
        this.news = news;
        this.newsOwner = newsOwner;
    }

    public String getNews() {
        return news;
    }

    public long getNewsOwner() {
        return newsOwner;
    }
}
