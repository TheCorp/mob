package com.mob.model.employer;

import java.util.HashSet;
import java.util.Set;


public class SearchCriteria {

	private Opportunity associatedOpportunity;
	private Set<Criteria> criterias = new HashSet<Criteria>();
	public Opportunity getAssociatedOpportunity() {
		return associatedOpportunity;
	}
	public void setAssociatedOpportunity(Opportunity associatedOpportunity) {
		this.associatedOpportunity = associatedOpportunity;
	}
	public Set<Criteria> getCriterias() {
		return criterias;
	}
	public void setCriterias(Set<Criteria> criterias) {
		this.criterias = criterias;
	}
}
