package com.mob.model.employer;

import java.util.HashSet;
import java.util.Set;


public class ResultCriteria {

	private SearchCriteria associatedSearchCriteria;
	private Set<Criteria> criterias = new HashSet<Criteria>();
	public SearchCriteria getAssociatedSearchCriteria() {
		return associatedSearchCriteria;
	}
	public void setAssociatedSearchCriteria(SearchCriteria associatedSearchCriteria) {
		this.associatedSearchCriteria = associatedSearchCriteria;
	}
	public Set<Criteria> getCriterias() {
		return criterias;
	}
	public void setCriterias(Set<Criteria> criterias) {
		this.criterias = criterias;
	}
}
