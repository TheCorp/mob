public class MOBConstants {

    /**
      * Default Constructor
      */
     private MOBConstants() {
     }

        /**
     * Log4j.properties path
     */
    private static String log4jPath;
    /**
     * Get the log4j path
     * @return log4jPath
     */
    public static String getLog4jPath() {
        return log4jPath;
    }
    /**
     * Set the log4j path
     * @param log4jPathArg - Log4j Path
     */
    public static void setLog4jPath(final String log4jPathArg) {
        log4jPath = log4jPathArg;
    }

}
